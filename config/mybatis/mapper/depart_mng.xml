<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="depart_mng">
	<!-- SQL문들을 작성하는 파일 -->
	<select id="getList" parameterType="String" resultType="business.depart_mng.Depart_Mng_DTO">
		select * from Depart_Mng where bmember_id = #{id}
	</select>
	
	<insert id="insert" parameterType="business.depart_mng.Depart_Mng_DTO">
		insert into Depart_Mng values(Depart_Mng_seq.nextval,#{bmember_id},#{name})
	</insert>
	
	<update id="update" parameterType="business.depart_mng.Depart_Mng_DTO">
		update Depart_Mng set name = #{name} where depart_mng_no = #{depart_mng_no}
	</update>
	
	<delete id="delete" parameterType="int">
		delete Depart_Mng where depart_mng_no = #{no}
	</delete>
	
</mapper>












